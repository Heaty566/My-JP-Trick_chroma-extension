{
        "manifest_version": 2,

        "name": "My JP Snippets",
        "description": "Quick pick up JP Type",
        "version": "1.0.0",
        "icons": { "128": "icon_128.png" },
        "browser_action": {
                "default_icon": "icon.png",
                "default_popup": "popup.html"
        },
        "permissions": ["activeTab", "https://code.jquery.com/jquery-3.5.1.min.js"],
        "content_security_policy": "script-src 'self' 'unsafe-eval' https://code.jquery.com/jquery-3.5.1.min.js; object-src 'self'"
}
